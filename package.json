  "main": "app.js",
  "scripts": {
    "build": "prisma generate --schema=./src/prisma/schema.prisma rimraf dist && tsc --build && tsc-alias",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "start:dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "start": "node -r tsconfig-paths/register app/dist/app.js",
    "start:prod": "node app/dist/app.js",
    "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --force --schema=./src/prisma/schema.prisma",
    "prisma:devmigrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
